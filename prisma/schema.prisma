generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}

model Users {
    id Int @id @default(autoincrement())
    firstName String
    lastName String
    age Int
    createdAt DateTime @default(now())
    updateAt DateTime @updatedAt
}

model House {
    id Int @id @default(autoincrement())
    
}